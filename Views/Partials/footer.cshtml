 
  @inherits UmbracoViewPage 

  @{
    
    var homePage = Model.AncestorOrSelf<HomePage>();

  }
  
 <!-- Footer-->

        <footer class="py-5 bg-black">
            <div class="container px-4 px-lg-5">
                <div class="row gx-4 gx-lg-5 justify-content-center">
                    <div class="col-md-10 col-lg-8 col-xl-7">
                        @if(homePage.FooterSocialLinks != null){
                            <ul class="list-inline text-center">
                                @foreach (var socialLink in homePage.FooterSocialLinks)
                                {
                                     <li class="list-inline-item">
                                <a href="@socialLink.Url">
                                    <span class="fa-stack fa-lg">
                                        <i class="fas fa-circle fa-stack-2x"></i>
                                        <i class="fab fa-@socialLink.Network fa-stack-1x fa-inverse"></i>
                                    </span>
                                </a>
                            </li>
                                }
                           
                           
                        </ul>

                        }
                        
                        <div class="container px-5"><p class="m-0 text-center text-white small">&copy; @Umbraco.GetDictionaryValue("Footer.CopyrightTitle") @DateTime.Now.Year</p></div>
                    </div>
                </div>
            </div>
        </footer>